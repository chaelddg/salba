    <script>

          //knob
          $(function() {
            $(".knob").knob({
              'draw' : function () {
                $(this.i).val(this.cv + '%')
              }
            })
          });

          //carousel
          $(document).ready(function() {
              $("#owl-slider").owlCarousel({
                  navigation : true,
                  slideSpeed : 300,
                  paginationSpeed : 400,
                  singleItem : true

              });
          });

          //custom select box

          $(function(){
              $('select.styled').customSelect();
          });

        /* ---------- Map ---------- */
      $(function(){
        $('#map').vectorMap({
          map: 'world_mill_en',
          series: {
            regions: [{
              values: gdpData,
              scale: ['#000', '#000'],
              normalizeFunction: 'polynomial'
            }]
          },
        backgroundColor: '#eef3f7',
          onLabelShow: function(e, el, code){
            el.html(el.html()+' (GDP - '+gdpData[code]+')');
          }
        });
      });

  </script>

  <script>
    $(document).ready(function() {
          var primus = new Primus('http://localhost:4000/');

          primus.on('user-notif', function(data) {
              console.log(data);

              switch(data) {
                case "request accepted":
                    console.log("**** CLIENT ACCEPT *******");
                    $('#notif-ul').append('<li><a href="/dashboard/reserved"><span class="label label-success"><i class="icon_book_alt"></i></span> '+data+'</a></li>');
                    $('#notif-count').text("1");
                break;

                case "request declined":
                    console.log("**** CLIENT DECLINE *******");
                    $('#notif-ul').append('<li><a href="/"><span class="label label-danger"><i class="icon_book_alt"></i></span> '+data+'</a></li>');
                    $('#notif-count').text("1");
                break;

                default:
                break;

              }

            });
    });

  </script>

